<!DOCTYPE html>
<html lang="zxx" class="no-js">

<head>
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon-->
    <link rel="shortcut icon" href="~/img/fav.png">
    <!-- Author Meta -->
    <meta name="author" content="CodePixar">
    <!-- Meta Description -->
    <meta name="description" content="">
    <!-- Meta Keyword -->
    <meta name="keywords" content="">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>@ViewData["Title"] </title>

    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i|Roboto:400,500" rel="stylesheet">
    <!--
            CSS
            ============================================= -->
    <link rel="stylesheet" href="~/css/linearicons.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/availability-calendar.css">
    <link rel="stylesheet" href="~/css/magnific-popup.css">
    <link rel="stylesheet" href="~/css/nice-select.css">
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" href="~/css/bootstrap.css">
    <link rel="stylesheet" href="~/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/css/main.css">
    <link rel="stylesheet" type="text/css" href="~/DataTables/datatables.css">
</head>

<body>
    <!--================ Start Header Area =================-->
    <header class="header-area">
        <div class="container">
            <div class="header-wrap">
                <div class="header-top d-flex justify-content-between align-items-center navbar-expand-md">
                    <div class="col menu-left">
                        <a class="active" asp-controller="Home" asp-action="Index">Home</a>
                        <a asp-controller="Home" asp-action="About">About</a>
                    </div>
                    <div class="col-3 logo">
                        <a asp-controller="Home" asp-action="Index"><img class="mx-auto" src="img/logo2.png" alt=""></a>
                    </div>
                    <nav class="col navbar navbar-expand-md justify-content-end">

                        <!-- Toggler/collapsibe Button -->
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
                            <span class="lnr lnr-menu"></span>
                        </button>

                        <!-- Navbar links -->
                        <div class="collapse navbar-collapse menu-right" id="collapsibleNavbar">
                            <ul class="navbar-nav justify-content-center w-100">
                                <li class="nav-item">
                                    <a class="nav-link" asp-controller="Recipe" asp-action="Recipes" >Recipes</a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Home" asp-action="Ingredients" class="nav-link">Ingredients</a>
                                </li>
                                <li class="nav-item">
                                    <a asp-controller="Home" asp-action="MyList" class="nav-link">My List</a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!--================ End Header Area =================-->

    <!--================ Start render body =================-->
    <div class="container">
        <main role="main" class="pb-3">
            <div id='cssmenu'>
                <ul>
                    <li><a asp-controller="Recipe" asp-action="Recipes"><span>Home</span></a></li>
                    <li class='active has-sub'>
                        <a href='#'><span>Keto Recipes</span></a>
                        <ul>
                            <li class='has-sub'>
                                <a href='#'><span>Entrees</span></a>
                                <ul>
                                    <li><a href='#'><span>Beef</span></a></li>
                                    <li><a href='#'><span>Chicken</span></a></li>
                                    <li><a href='#'><span>Fish</span></a></li>
                                    <li><a href='#'><span>Pork</span></a></li>
                                    <li class='last'><a href='#'><span>Other</span></a></li>
                                </ul>
                            </li>
                            <li class='has-sub'>
                                <a href='#'><span>Sides</span></a>
                                <ul>
                                    <li><a href='#'><span>Sub Product</span></a></li>
                                    <li class='last'><a href='#'><span>Sub Product</span></a></li>
                                </ul>
                            </li>
                            <li class='has-sub'>
                                <a href='#'><span>Desserts</span></a>
                                <ul>
                                    <li><a href='#'><span>Sub Product</span></a></li>
                                    <li class='last'><a href='#'><span>Sub Product</span></a></li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li><a asp-controller="Recipe" asp-action="Entrees"><span>Entrees</span></a></li>
                    <li><a asp-controller="Recipe" asp-action="Sides"><span>Sides</span></a></li>
                    <li class='last'><a asp-controller="Recipe" asp-action="Desserts"><span>Dessert</span></a></li>
                </ul>
            </div>
            @RenderBody()
        </main>
    </div>
    <!--================ End render body=================-->
    <!--================ Start Footer Area =================-->
    <footer style="bottom:0; position:fixed; width:100%;" class="footer-area">
        <p class="footer-text m-0 col-lg-6 col-md-12">Copyright © 2018 All rights reserved | This template is made with
        <span class="lnr lnr-heart"></span> by <a >Colorlib</a></p>
    </footer>
    <!--================ End Footer Area =================-->

    <script src="~/js/vendor/jquery-2.2.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
            crossorigin="anonymous"></script>
    <script src="~/js/vendor/bootstrap.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.sticky.js"></script>
    <script src="~/js/parallax.min.js"></script>
    <script src="~/js/jquery.nice-select.min.js"></script>
    <script src="~/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBhOdIF3Y9382fqJYt5I_sswSrEw5eihAA"></script>
    <script src="~/js/bootstrap-datepicker.js"></script>
    <script src="~/js/main.js"></script>
    <script type="text/javascript" charset="utf8" src="~/DataTables/datatables.js"></script>
    <script>
        ( function( $ ) {
            $( document ).ready(function() {
            $('#cssmenu li.has-sub>a').on('click', function(){
                    $(this).removeAttr('href');
                    var element = $(this).parent('li');
                    if (element.hasClass('open')) {
                        element.removeClass('open');
                        element.find('li').removeClass('open');
                        element.find('ul').slideUp();
                    }
                    else {
                        element.addClass('open');
                        element.children('ul').slideDown();
                        element.siblings('li').children('ul').slideUp();
                        element.siblings('li').removeClass('open');
                        element.siblings('li').find('li').removeClass('open');
                        element.siblings('li').find('ul').slideUp();
                    }
                });
            });
        } )( jQuery );
    </script>
    @RenderSection("Scripts", required: false)
</body>

</html>