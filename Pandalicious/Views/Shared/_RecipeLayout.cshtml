@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers;
<!DOCTYPE html>
<html lang="zxx" class="no-js">


<head>
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>@ViewData["Title"] </title>
    !--====================== CSS and Scripts ======================= -->
    <link rel="stylesheet" type="text/css" href="~/DataTables/datatables.css">
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/recipe.css" />
    <link rel="stylesheet" href="~/css/main.css" />
    <link href="https://fonts.googleapis.com/css?family=Great+Vibes&display=swap" rel="stylesheet">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.7.8/angular.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i|Roboto:400,500" rel="stylesheet">
    <!--
            CSS
            ============================================= -->
    <link rel="stylesheet" href="~/css/linearicons.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/availability-calendar.css">
    <link rel="stylesheet" href="~/css/magnific-popup.css">
    <link rel="stylesheet" href="~/css/nice-select.css">
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" href="~/css/bootstrap.css">
    <link rel="stylesheet" href="~/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/css/main.css">
    <link rel="stylesheet" type="text/css" href="~/DataTables/datatables.css">
</head>

<body>
    <!--================ Start Header Area =================-->
    <div class="fixed-top navigationPane">
        <nav class="navbar navbar-expand-md">
            <div class="col-lg-5 col-md-5 col-sm-4 menu-items">
                <a class="navbar-brand link" asp-controller="Home" asp-action="Index">Home</a>
                <a class="navbar-brand" asp-controller="Home" asp-action="About">About</a>
                <a class="navbar-brand" href="#">Misc.</a>
            </div>
            <div clas="col-lg-2 col-md-2 col-sm-2 logo">
                <img id="pandaPic" src="~/img/panda.jpg" alt="Panda picture" />
            </div>
            <div class="col-lg-5 col-md-5 col-sm-5 menu-items">
                <a class="navbar-brand" asp-controller="Recipe" asp-action="Recipes">Recipes</a>
                <a class="navbar-brand" asp-controller="Home"   asp-action="Ingredients">Ingredients</a>
                <a class="navbar-brand" asp-controller="List"   asp-action="List">My List</a>
            </div>
        </nav>
    </div> 
    <!--================ End Header Area =================-->
    <!--================ Start render body =================-->
        <main role="main" class="pb-3">
            <div id='cssmenu'>
                <ul>
                    <li><a asp-controller="Recipe" asp-action="Recipes"><span>Home</span></a></li>
                    <li class="active has-sub">
                        <a><span>Entrees</span></a>
                        <ul>
                            <li><a asp-controller="Recipe" asp-action="Entrees"><span>All Entrees</span></a></li>
                            <li><a asp-controller="Recipe" asp-action="BeefEntrees"><span>Beef</span></a></li>
                            <li><a asp-controller="Recipe" asp-action="ChickenEntrees"><span>Chicken</span></a></li>
                            <li><a asp-controller="Recipe" asp-action="FishEntrees"><span>Fish</span></a></li>
                            <li><a asp-controller="Recipe" asp-action="PorkEntrees"><span>Pork</span></a></li>
                            <li class='last'><a asp-controller="Recipe" asp-action="OtherEntrees"><span>Other</span></a></li>
                        </ul>
                    </li>
                    <li><a asp-controller="Recipe" asp-action="Sides"><span>Sides</span></a></li>
                    <li class='last'><a asp-controller="Recipe" asp-action="Desserts"><span>Dessert</span></a></li>
                </ul>
            </div>
            @RenderBody()
        </main>
    <!--================ End render body=================-->
    <!--================ Start Footer Area =================-->
    <footer>
        <p class="footer-text m-0 col-lg-12 col-md-12 col-sm-12"><i>Pandalicious &#169; 2019</i></p>
    </footer>
    <!--================ End Footer Area =================-->

    <script src="~/js/vendor/jquery-2.2.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
            crossorigin="anonymous"></script>
    <script src="~/js/vendor/bootstrap.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.sticky.js"></script>
    <script src="~/js/parallax.min.js"></script>
    <script src="~/js/jquery.nice-select.min.js"></script>
    <script src="~/js/jquery.ajaxchimp.min.js"></script>
    <script src="~/js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBhOdIF3Y9382fqJYt5I_sswSrEw5eihAA"></script>
    <script src="~/js/bootstrap-datepicker.js"></script>
    <script src="~/js/main.js"></script>
    <script type="text/javascript" charset="utf8" src="~/DataTables/datatables.js"></script>
    <script>(function ($) {
            $(document).ready(function () {
                $('#cssmenu li.has-sub>a').on('click', function () {
                    $(this).removeAttr('href');
                    var element = $(this).parent('li');
                    if (element.hasClass('open')) {
                        element.removeClass('open');
                        element.find('li').removeClass('open');
                        element.find('ul').slideUp();
                    }
                    else {
                        element.addClass('open');
                        element.children('ul').slideDown();
                        element.siblings('li').children('ul').slideUp();
                        element.siblings('li').removeClass('open');
                        element.siblings('li').find('li').removeClass('open');
                        element.siblings('li').find('ul').slideUp();
                    }
                });
            });
        })(jQuery);</script>
    @RenderSection("Scripts", required: false)
</body>

</html>